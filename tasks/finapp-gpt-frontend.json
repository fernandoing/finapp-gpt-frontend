{
  "containerDefinitions":
  [
    {
        "name": "finapp-gpt-frontend",
        "image": "nginx",
        "essential": true,
        "portMappings": [
            {
                "containerPort": 8080,
                "hostPort": 8080,
                "protocol": "tcp"
            }
        ],
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "/ecs/finapp-gpt-frontend",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "application"
          }
      },
        "environment": [
            {
                "name": "PLACEHOLDER",
                "value": "REPLACEME"
            }
        ]
    }
  ]
}
